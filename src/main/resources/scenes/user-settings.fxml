            <Insets bottom="5.0" left="10.0" top="5.0" />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <TableView fx:id="usersTableView" prefHeight="410.0" prefWidth="638.0" stylesheets="@style/nonullrow-tableview.css">
        <columns>
          <TableColumn fx:id="usernameTableColumn" prefWidth="179.19997864961624" text="Username" />
          <TableColumn fx:id="passwordTableColumn" prefWidth="244.00001525878906" text="Password" />
            <TableColumn fx:id="modifyTableColumn" minWidth="0.0" prefWidth="98.40000915527344" text="Modify" />
            <TableColumn fx:id="deleteTableColumn" prefWidth="116.79998779296875" text="Delete" />
        </columns>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </TableView>
   </children>
</VBox>
